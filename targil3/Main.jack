class Main {
    function void main() {
        var SpaceShip spaceShip; 

        var char key;
        var boolean exit;

        let exit = false;
        let spaceShip = SpaceShip.new();
        while(~exit) {
            while(Keyboard.keyPressed() = 130) {
                do spaceShip.moveLeft();
            }
            while(Keyboard.keyPressed() = 132) {
                do spaceShip.moveRight();
            }
        }
        return;
    }
